{
  "expo": {
    "name": "mirCar",
    "slug": "mirCar",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mirCar",
      "config": {
        "googleMapsApiKey": "AIzaSyDnnDG1NHeMAI9YxzJBqo2kg0fz7gSCIlI"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },

      "package": "com.mirCar",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDnnDG1NHeMAI9YxzJBqo2kg0fz7gSCIlI"
        }
      },
      "permissions": ["ACCESS_COARSE_LOCATION", "ACCESS_FINE_LOCATION", "FOREGROUND_SERVICE"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
